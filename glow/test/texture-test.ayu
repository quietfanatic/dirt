[ayu::Document {
    image: [glow::UniqueImage {
        size: [7 5]
        pixels: [
            &p: [0x26 0x74 0xdb 0xf0]
            *p *p *p *p *p *p *p
            *p *p *p *p *p *p *p
            *p *p *p *p *p *p *p
            *p *p *p *p *p *p *p
            *p *p *p *p *p *p *p
        ]
    }]
    texture: [glow::ImageTexture {
        image: #/image+1
        filter: GL_LINEAR
    }]
    file_image: [glow::FileImage {
        source: image.png
    }]
    texture2: [glow::ImageTexture {
        image: #/file_image+1
    }]
}]
